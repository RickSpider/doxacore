<?page title="report" contentType="text/html;charset=UTF-8"?>
<zk>

	<window title="Reporte" border="normal"
		position="center,center" closable="true" mode="modal"
		action="show: slideDown;hide: slideUp">
		
		<jasperreport id="report" height="360px"
        src="@load(vm.reportConfig.source)"
        type="pdf"
        datasource="@load(vm.reportConfig.dataSource)"/>
	
		
	</window>
</zk>